@model List<ObavijestiVM>

@foreach (var item in Model)
{
    <div class="col-md-5 m-md-1">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-8">
                <h5 class="card-title m-md-3">@item.Naslov</h5>
                <p class="card-text p-3">@item.Obavijest</p>
            </div>
        </div>
    </div>
}


